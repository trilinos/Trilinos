<ParameterList name="Example 01">
  <Parameter name="Derivative Check"        type="bool"     value="true"  />
  <!--================================================================================--> 
  <!-- Physical Parameters                                                            --> 
  <Parameter name="Diffusion Scale"         type="double"   value="0.5"   />
  <Parameter name="Reaction Scale"          type="double"   value="1.0"   />
  <Parameter name="Reaction Type"           type="int"      value="1"     />
  <!--================================================================================--> 
  <!-- Space Discretization                                                           --> 
  <Parameter name="Spatial Discretization"  type="int"      value="64"    />
  <!--================================================================================-->
  <!--================================================================================--> 
  <!-- Time Discretization                                                            --> 
  <Parameter name="End Time"                type="double"   value="1.0"   />
  <Parameter name="Temporal Discretization" type="int"      value="128"   />
  <Parameter name="Integration Factor"      type="double"   value="0.5"   />
  <!--================================================================================-->
  <!-- Objective Function                                                             --> 
  <Parameter name="Target Type"             type="int"      value="2"     />
  <Parameter name="Control Penalty"         type="double"   value="1e-3"  />
  <!--================================================================================--> 
  <!-- Sketching Parameters                                                           --> 
  <ParameterList name="Reduced Dynamic Objective">
    <Parameter name="Use Sketching"                 type="bool"     value="false" />
    <Parameter name="State Rank"                    type="int"      value="10"    />
    <Parameter name="Adjoint Rank"                  type="int"      value="10"    />
    <Parameter name="State Sensitvity Rank"         type="int"      value="10"    />
    <Parameter name="Use Hessian"                   type="bool"     value="true"  />
    <Parameter name="Use Only Sketched Sensitivity" type="bool"     value="true"  />
  </ParameterList>
  <!--================================================================================-->
  <!--================================================================================--> 
  <!-- MGRIT Parameters                                                               --> 
  <Parameter name="MGRIT Sweeps"                    type="int"      value="1"     />
  <Parameter name="MGRIT Relaxation"                type="double"   value="0.6666"/>
  <Parameter name="MGRIT Levels"                    type="int"      value="3"     />
  <Parameter name="MGRIT Krylov Relative Tolerance" type="double"   value="1e-4"     />
</ParameterList>


