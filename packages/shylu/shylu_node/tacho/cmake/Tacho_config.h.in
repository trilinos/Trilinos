/* Define if want to build with size_type (size_t)  enabled */
#cmakedefine TACHO_USE_INT_SIZE_T

/* Define if want to build with MKL enabled */
#cmakedefine TACHO_HAVE_MKL

/* Define if want to build with LAPACK enabled */
