#ifdef HAVE_TPETRA_DEFAULTNODE_SERIALWRAPPERNODE
#  define HAVE_KOKKOSCLASSIC_DEFAULTNODE_SERIALWRAPPERNODE 1
#endif // HAVE_TPETRA_DEFAULTNODE_SERIALWRAPPERNODE

#endif /* TPETRACLASSIC_DEFAULTNODE_CONFIG_H */
