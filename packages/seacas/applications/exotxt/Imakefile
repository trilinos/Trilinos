CODE = exotxt

SRCS = \
dbicon.f dbielb.f dbign.f  dbinam.f  dbiqa.f  dbiste.f \
dbivtt.f dbpini.f dbvini.f exotxt2.f rwpval.f wrelb.f  \
wress.f  wrinit.f wrmap.f  wrname.f  wrnps.f  wrqa.f   \
wrstep.f wrxyz.f

OBJS = ${SRCS:.f=.o}

LIBS = $(EXOIIV2FOR) $(EXOIIV2C) $(NETCDF) $(SUPLIB) $(SUPES)

NormalFortranTarget($(CODE),$(OBJS),NullParameter,NullParameter,$(LIBS))

InstallTarget(install,$(CODE),$(INSTBINFLAGS),$(BINDIR))

DependTarget()

