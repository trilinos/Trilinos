echo "begin testwt1" >> test.output
${PREFIX} ${SRCDIR}/testwt1 >> test.output
echo "end testwt1" >> test.output
${NCDUMP} -d5,5 test.exo | grep -v version | ${DIFF} - ${SRCDIR}/test1.dmp | tee testwt1.res

