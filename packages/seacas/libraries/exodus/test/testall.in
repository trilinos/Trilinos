echo "end testwt-zeroe" >> test.output
${NCDUMP} -d5,5 test.exo | grep -v version | grep -v _FillValue | ${DIFF} - ${SRCDIR}/testwt-zeroe.dmp | tee testwt-zeroe.res

echo "testrd - read test of file with zero elements..."
