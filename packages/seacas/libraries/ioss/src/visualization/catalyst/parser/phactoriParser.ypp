namespace Iovs {

void parsePhactoriString(const std::string & s,
                         Iovs::PhactoriParserInterface::ParseInfo & pinfo)
{
  ParseData pd;
  pd.nodeVars = pinfo.nodeVars;
  pd.elementVars = pinfo.elementVars;
  pd.globalVars = pinfo.globalVars;
  pd.checkNodeIDRange = pinfo.checkNodeIDRange;
  pd.nodeIDRange = pinfo.nodeIDRange;
  pd.checkElementIDRange = pinfo.checkElementIDRange;
  pd.elementIDRange = pinfo.elementIDRange;
  pd.separator = pinfo.separator;

  std::string pps = preProcessPhactoriInput(s);

  yyscan_t myscanner;
  catalystlex_init(&myscanner);
  YY_BUFFER_STATE buffer = catalyst_scan_string(pps.c_str(), myscanner);
  int ret = yyparse(myscanner, &pd);
  catalyst_delete_buffer(buffer, myscanner);
  catalystlex_destroy(myscanner);
  Json::StreamWriterBuilder builder;
  builder["indentation"] = "";
  pinfo.jsonParseResult = Json::writeString(builder, pd.root);
  pinfo.parseFailed = ret;
}

