void switch_variable_to_used(const std::string& name,
                             Iovs::PhactoriParserInterface::varMap& v)
{
  if(v.find(name) != v.end())
   {
   if( v[name] == Iovs::PhactoriParserInterface::ALLTYPE )
     v[name] = Iovs::PhactoriParserInterface::ALLTYPE_USED;
   else if( v[name] == Iovs::PhactoriParserInterface::TENSOR )
     v[name] = Iovs::PhactoriParserInterface::TENSOR_USED;
   else if( v[name] == Iovs::PhactoriParserInterface::SCALAR )
     v[name] = Iovs::PhactoriParserInterface::SCALAR_USED;
   else if( v[name] == Iovs::PhactoriParserInterface::VECTOR )
     v[name] = Iovs::PhactoriParserInterface::VECTOR_USED;
   }
}

bool compare_variable_type(const std::string& name, 
                           Iovs::PhactoriParserInterface::varMap& v,
                           Iovs::PhactoriParserInterface::VarType vt)
{
  if(v.find(name) != v.end())
   {
   if( v[name] == vt || 
       v[name] == Iovs::PhactoriParserInterface::ALLTYPE || 
       v[name] == Iovs::PhactoriParserInterface::ALLTYPE_USED )
     return true;
   else if( v[name] == Iovs::PhactoriParserInterface::SCALAR_USED &&
             vt == Iovs::PhactoriParserInterface::SCALAR )
     return true;
   else if( v[name] == Iovs::PhactoriParserInterface::VECTOR_USED &&
             vt == Iovs::PhactoriParserInterface::VECTOR )
     return true;
   else if( v[name] == Iovs::PhactoriParserInterface::TENSOR_USED && 
             vt == Iovs::PhactoriParserInterface::TENSOR )
     return true;
   }
  return false;
}

bool check_variable_name(YYLTYPE * llocp,
                         yyscan_t yyscanner,
                         ParseData& pd,
                         const std::string& name, 
                         Iovs::PhactoriParserInterface::VarType vt)
{
  if (!pd.nodeVars.empty() && !pd.elementVars.empty() && !pd.globalVars.empty())
    {
    bool is_node_var = (pd.nodeVars.find(name) != pd.nodeVars.end());
    bool is_element_var = (pd.elementVars.find(name) != pd.elementVars.end());
    bool is_global_var = (pd.globalVars.find(name) != pd.globalVars.end());

    if( !is_node_var &&
        !is_global_var &&
        !is_element_var )
      {
      yyerror(llocp, yyscanner, &pd, ("Error: variable name not found: " + name).c_str());
      return false;
      }

    bool found_match = false;

    if(is_node_var)
      {
      if( compare_variable_type(name, pd.nodeVars, vt) )
        {
        switch_variable_to_used(name, pd.nodeVars);
        found_match = true;
        }
      }

    if(is_global_var)
      {
      if( compare_variable_type(name, pd.globalVars, vt) )
        {
        switch_variable_to_used(name, pd.globalVars);
        found_match = true;
        }
      }

    if(is_element_var)
      {
      if( compare_variable_type(name, pd.elementVars, vt) )
        {
        switch_variable_to_used(name, pd.elementVars);
        found_match = true;
        }
      }

    if(!found_match)
      {
      std::string t = "scalar";
      if(vt == Iovs::PhactoriParserInterface::VECTOR)
        t = "vector";
      else if(vt == Iovs::PhactoriParserInterface::TENSOR)
        t = "tensor";
      yyerror(llocp, yyscanner, &pd, ("Error: variable is not " + t + " type: " + name).c_str());
      return false;
      }

    }

  return true;
}

bool check_vector_component(YYLTYPE * llocp,
                            yyscan_t yyscanner,
                            ParseData& pd,
                            const std::string& name)
{
  std::string sep = pd.separator;
  unsigned int offset = sep.size() + 1;
  if(name.size() > offset)
    {
    std::string term  = name.substr(name.size() - offset);
    if(term == sep + "X" || term == sep + "Y" || term == sep + "Z" ||
       term == sep + "x" || term == sep + "y" || term == sep + "z")
      return check_variable_name(llocp, yyscanner, pd, name.substr(0,name.size() - offset),
          Iovs::PhactoriParserInterface::VECTOR);
    }

  yyerror(llocp, yyscanner, &pd, ("Error: invalid vector name component (must be x, y, or z) " + name).c_str());
  return false;
}

bool check_tensor_component(YYLTYPE * llocp,
                            yyscan_t yyscanner,
                            ParseData& pd,
                            const std::string& name)
{
  std::string sep = pd.separator;
  unsigned int offset = sep.size() + 2;
  if(name.size() > offset)
    {
    std::string term  = name.substr(name.size() - offset);
    if(term == sep + "XX" || term == sep + "XY" || term == sep + "XZ" ||
       term == sep + "YX" || term == sep + "YY" || term == sep + "YZ" ||
       term == sep + "ZX" || term == sep + "ZY" || term == sep + "ZZ" ||
       term == sep + "xx" || term == sep + "xy" || term == sep + "xz" ||
       term == sep + "yx" || term == sep + "yy" || term == sep + "yz" ||
       term == sep + "zx" || term == sep + "zy" || term == sep + "zz")
      return check_variable_name(llocp, yyscanner, pd, name.substr(0,name.size() - offset),
          Iovs::PhactoriParserInterface::TENSOR);
    }

  yyerror(llocp, yyscanner, &pd, ("Error: invalid tensor name component (must be xx, yy, zz, zx, zy, xy, or yz) " + name).c_str());
  return false;
}

bool check_catalyst_shortcuts(YYLTYPE * llocp,
                              yyscan_t yyscanner,
                              ParseData& pd)
{
  if( pd.ims_shortcut.size() &&
     (pd.root["representation blocks"].size() ||
      pd.root["imageset blocks"].size() ||
      pd.root["camera blocks"].size() ||
      pd.root["operation blocks"].size() ||
      pd.root["scatter plot blocks"].size() ||
      pd.root["plot over time blocks"].size()) )
    {
    yyerror(llocp, yyscanner, &pd, "Error: catalyst block shortcuts used with declared blocks.");
    return false;
    }

  bool ret = true;
  if(pd.ims_shortcut.size())
    {
    pd.root["imageset blocks"]["\\#ImageSet\\$ Shortcut"] = pd.ims_shortcut;
    ret = ret && check_representation(llocp, yyscanner, pd, pd.ims_shortcut);
    ret = ret && check_camera(llocp, yyscanner, pd, pd.ims_shortcut);
    }
  return ret;
}

