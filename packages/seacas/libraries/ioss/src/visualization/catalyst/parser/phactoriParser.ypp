    Iovs::PhactoriParserInterface::varMap nodeVars;
    Iovs::PhactoriParserInterface::varMap elementVars;
    Iovs::PhactoriParserInterface::varMap globalVars;
    bool checkNodeIDRange;
    Iovs::PhactoriParserInterface::idRange nodeIDRange;
    bool checkElementIDRange;
    Iovs::PhactoriParserInterface::idRange elementIDRange;
