  std::vector<std::string> getTokensBeforeEqualsAsLowerCase(
      const std::string & line);
  std::string getStringAfterEquals(const std::string & line);
  std::string getStringBeforeEquals(const std::string & line);
  void toLowerCase(std::string & s);
  bool stringStartsAndEndsWithQuotes(const std::string & s);
  bool containsPhactoriCommandThatNeedsQuotes(std::vector<std::string>& tl);
  void addQuotesToString(std::string & s);
  std::string trim(const std::string& s);

  std::string preProcessPhactoriInput(const std::string & input) {
      std::istringstream iss(input);

      std::string result;
      for (std::string line; std::getline(iss, line); ) {
          std::vector<std::string> tl = getTokensBeforeEqualsAsLowerCase(line);
          std::string ae = getStringAfterEquals(line);
          if(containsPhactoriCommandThatNeedsQuotes(tl) &&
              !stringStartsAndEndsWithQuotes(ae)) {
              addQuotesToString(ae);
              result += (getStringBeforeEquals(line) + "=" + ae + "\n");
          }
          else {
              result += line + "\n";
          }
      }
      return result;
  }

  std::vector<std::string> getTokensBeforeEqualsAsLowerCase(
      const std::string & line) {

      std::vector<std::string> tl;
      std::istringstream iss(line);
      std::string token;
      while(iss >> token) {
         if (token == "=") {
             break;
         }
         else {
             toLowerCase(token);
             tl.push_back(token);
         }
      }
      return tl;
  }

  std::string getStringAfterEquals(const std::string & line) {
      return line.substr(line.find("=") + 1);
  }

  std::string getStringBeforeEquals(const std::string & line) {
      return line.substr(0, line.find("="));
  }

  void toLowerCase(std::string & s) {
      std::transform(s.begin(), s.end(), s.begin(), ::tolower);
  }

  bool stringStartsAndEndsWithQuotes(const std::string & s) {
      size_t first = s.find_first_not_of(' ');
      bool startWithQuote = s.rfind("\"", first) == first;
      size_t last = s.find_last_not_of(' ');
      bool endsWithQuote = s.rfind("\"", last) == last;
      return startWithQuote && endsWithQuote;
  }

  void addQuotesToString(std::string & s) {
      s = "\"" + trim(s) + "\"";
  }

  std::string trim(const std::string& s) {
    size_t first = s.find_first_not_of(' ');
    if (std::string::npos == first) {
        return s;
    }
    size_t last = s.find_last_not_of(' ');
    return s.substr(first, (last - first + 1));
  }

  bool containsPhactoriCommandThatNeedsQuotes(std::vector<std::string>& tl) {
      if (tl.size() == 1 && tl[0] == "function") {
          return true;
      }
      else if (tl.size() == 2 && (tl[0] == "plot" || tl[0] == "image") &&
          tl[1] == "basename") {
          return true;
      }
      else if (tl.size() == 2 && (tl[0] == "plot" || tl[0] == "image") &&
          tl[1] == "basedirectory") {
          return true;
      }
      else if (tl.size() == 3 && tl[0] == "image" && tl[1] == "name" &&
          tl[2] == "addon") {
          return true;
      }
      else if (tl.size() == 4 && (tl[0] == "x" || tl[0] == "y" ||
          tl[0] == "z") && tl[1] == "axis" && tl[2] == "label" &&
          tl[3] == "name") {
          return true;
      }
      return false;
  }

} // End anonymous namespace

