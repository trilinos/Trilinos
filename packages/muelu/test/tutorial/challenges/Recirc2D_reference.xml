<ParameterList name="MueLu">

  <Parameter name="verbosity" type="string" value="high"/>

  <Parameter name="max levels" type="int" value="3"/>
  <Parameter name="coarse: max size" type="int" value="500"/>

  <Parameter name="multigrid algorithm" type="string" value="pg"/>
  <Parameter name="problem: symmetric" type="bool" value="false"/>
  <Parameter name="transpose: use implicit" type="bool" value="false"/>
  <Parameter name="sa: damping factor"  type="double" value="0.5"/>

  <Parameter name="smoother: type" type="string" value="RELAXATION"/>
  <ParameterList name="smoother: params">
    <Parameter name="relaxation: type"  type="string" value="Gauss-Seidel"/>
    <Parameter name="relaxation: sweeps" type="int" value="1"/>
    <Parameter name="relaxation: damping factor" type="double" value="1.7"/>
  </ParameterList>

  <!-- Aggregation -->
  <Parameter name="aggregation: type" type="string" value="uncoupled"/>
  <Parameter name="aggregation: min agg size" type="int" value="10"/>
  <Parameter name="aggregation: max agg size" type="int" value="12"/>

</ParameterList>
