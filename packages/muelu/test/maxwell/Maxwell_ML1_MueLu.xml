<ParameterList name="MueLu">
  <Parameter name="parameterlist: syntax" type="string" value="ml"/>
  <Parameter name="default values" type="string" value="maxwell"/>
  <Parameter name="ML output" type="int" value="10"/>
  <Parameter name="ML print final list" type="int" value="0"/>
  <Parameter name="increasing or decreasing" type="string" value="decreasing"/>

  <Parameter name="aggregation: type" type="string" value="Uncoupled"/>
  <Parameter name="aggregation: threshold" type="double" value="0.01"/>
  <Parameter name="aggregation: aux: enable" type="bool" value="false"/>
  <Parameter name="aggregation: edge prolongator drop threshold" type="double" value="0.0"/>
  <Parameter name="aggregation: damping factor" type="double" value="1.333"/>

  <Parameter name="smoother: type" type="string" value="Hiptmair"/>
  <Parameter name="smoother: sweeps" type="int" value="1"/>
  <Parameter name="smoother: damping factor" type="double" value="1"/>
  <Parameter name="smoother: pre or post" type="string" value="both"/>
  <Parameter name="smoother: Hiptmair efficient symmetric" type="bool" value="true"/>
  <Parameter name="subsmoother: type" type="string" value="symmetric Gauss-Seidel"/>
  <Parameter name="subsmoother: Chebyshev alpha" type="double" value="20"/>
  <Parameter name="subsmoother: node sweeps" type="int" value="8"/>
  <Parameter name="subsmoother: edge sweeps" type="int" value="8"/>

  <Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
  <Parameter name="coarse: max size" type="int" value="1000"/>

  <Parameter name="eigen-analysis: type" type="string" value="cg"/>

  <Parameter name="repartition: enable" type="int" value="0"/>
  <Parameter name="repartition: partitioner" type="string" value="Zoltan"/>
  <Parameter name="repartition: min per proc" type="int" value="1000"/>
  <Parameter name="repartition: Zoltan dimensions" type="int" value="3"/>
</ParameterList>
