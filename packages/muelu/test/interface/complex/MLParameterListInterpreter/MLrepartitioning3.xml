<ParameterList name="MultiLevelPreconditioner">
  <Parameter name="ML output"                   type="int"      value="8"/>
  <Parameter name="max levels"                  type="int"      value="5"/>
  <Parameter name="smoother: type"              type="string"   value="symmetric Gauss-Seidel"/>
  <Parameter name="smoother: sweeps"            type="int"      value="2"/>
  <Parameter name="coarse: type"                type="string"   value="Amesos-KLU"/>
  <Parameter name="repartition: enable"         type="int"      value="1"/>
  <Parameter name="repartition: max min ratio"  type="double"   value="1.2"/>
  <Parameter name="repartition: min per proc"   type="int"      value="256"/>
  <Parameter name="repartition: start level"    type="int"      value="2"/>
  <Parameter name="repartition: partitioner"    type="string"   value="Zoltan"/><!-- has no meaning for MueLu, necessary for ML -->
</ParameterList>
